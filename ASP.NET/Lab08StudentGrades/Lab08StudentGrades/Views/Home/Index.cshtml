@model IEnumerable<Lab08StudentGrades.Models.Entities.Student>

@{
    ViewData["Title"] = "Index";
}

<h2>Full Student List</h2>

<table class="table">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.ENumber)
                </th>
